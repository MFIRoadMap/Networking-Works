exit


ip nat pool NAT_POOL_R2 70.0.20.226 70.0.20.240 netmask 255.255.255.224
access-list 5 permit 0.0.0.0 255.255.255.255 
ip nat inside source list 5 pool NAT_POOL_R2 overload
int fa 0/0
ip nat outside
exit
int fa 1/0
ip nat inside 
exit
int fa 1/1
ip nat inside 
exit


