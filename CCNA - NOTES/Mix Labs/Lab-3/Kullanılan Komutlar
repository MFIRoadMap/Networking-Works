ESW1
--------------------------------------
en
conf ter
hostname ESW1

exit
vlan database 
vlan 10 name Personel
vlan 20 name Yazıcı
vlan 30 name Misafir
exit
conf ter

int vlan 10
ip address 192.168.10.2 255.255.255.0
vrrp 1 ip 192.168.10.1
vrrp 1 preempt
no sh 
exit
int vlan 20
ip address 192.168.20.2 255.255.255.0
vrrp 2 ip 192.168.20.1
vrrp 2 preempt
no sh 
exit
int vlan 30
ip address 192.168.30.2 255.255.255.0
vrrp 3 ip 192.168.30.1
vrrp 3 preempt
no sh 
exit

int fa 1/12 
switchport mode trunk
switchport trunk encapsulation dot1q
exit
int fa 1/13 
switchport mode trunk
switchport trunk encapsulation dot1q
exit
int fa 1/14 
switchport mode trunk
switchport trunk encapsulation dot1q
exit
int fa 1/15 
switchport mode trunk
switchport trunk encapsulation dot1q
exit
ip routing 


int fa 1/10
channel-group 2 mode on
exit
int fa 1/11 
channel-group 2 mode on
exit

int port-channel 2 
switchport mode trunk
exit


int fa 0/0
ip address 10.0.10.2 255.255.255.0
no sh
exit
int fa 0/1
ip address 20.0.10.2 255.255.255.0
no sh
exit

router ospf 5
network 10.0.10.2 0.0.0.255 area 100
network 20.0.10.2 0.0.0.255 area 100
network 192.168.10.1 0.0.0.255 area 100
network 192.168.20.1 0.0.0.255 area 100
network 192.168.30.1 0.0.0.255 area 100
exit




