Kullanılan Komutlar


R1
----------------------------------------
en
conf ter
hostname R1

int fa 0/0
ip address 10.0.0.1 255.255.255.0
no sh
exit
int fa 2/1
ip address 192.168.10.1 255.255.255.0
no sh
exit

router ospf 5
network 10.0.0.1 0.0.0.255 area 10
network 192.168.10.1 0.0.0.255 area 10
exit


crypto isakmp policy 20
authentication pre-share
encryption aes 256
group 14
hash sha256
exit
crypto isakmp key 0 R1_R3Ipsec address 20.0.0.3

crypto ipsec transform-set TS1 esp-aes esp-sha-hmac
mode tunnel
exit
access-list 105 permit ip 192.168.10.0 0.0.0.255 192.168.50.0 0.0.0.255 


crypto map CM1 5 ipsec-isakmp
match address 105
set transform-set TS1
set peer 20.0.0.3
exit

int fa 0/0
crypto map CM1
exit




R2
----------------------------------------
en
conf ter
hostname R2

int fa 0/0
ip address 20.0.0.2 255.255.255.0
no sh
exit
int fa 1/0
ip address 10.0.0.2 255.255.255.0
no sh
exit
int fa 2/1
ip address 192.168.20.1 255.255.255.0
no sh
exit

router ospf 5 
network 20.0.0.2 0.0.0.255 area 10
network 10.0.0.2 0.0.0.255 area 10
network 192.168.20.1 0.0.0.255 area 10
exit




R3
----------------------------------------
en
conf ter
hostname R3

int fa 0/0
ip address 30.0.0.3 255.255.255.0
no sh
exit
int fa 1/0
ip address 20.0.0.3 255.255.255.0
no sh
exit
int fa 2/1
ip address 192.168.30.1 255.255.255.0
no sh
exit

router ospf 5
network 30.0.0.3 0.0.0.255 area 10
network 20.0.0.3 0.0.0.255 area 10
network 192.168.30.1 0.0.0.255 area 10
exit


crypto isakmp policy 20
authentication pre-share
encryption aes 256
group 14
hash sha256
exit
crypto isakmp key 0 R1_R3Ipsec address 10.0.0.1

crypto ipsec transform-set TS1 esp-aes esp-sha-hmac
mode tunnel
exit
access-list 105 permit ip 192.168.50.0 0.0.0.255 192.168.10.0 0.0.0.255 

crypto map CM1 5 ipsec-isakmp
match address 105
set transform-set TS1
set peer 10.0.0.1
exit

int fa 1/0
crypto map CM1
exit


ip nat pool NAT_POOL 30.0.0.226 30.0.0.240 netmask 255.255.255.224 
access-list 10 permit 192.168.10.0 0.0.0.255
ip nat inside source list 10 pool NAT_POOL overload
int fa 1/0 
ip nat inside 
exit
int fa 0/0
ip nat outside
exit







