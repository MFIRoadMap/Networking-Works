R5
----------------------------------------
en
conf ter
hostname R5

int fa 1/0
ip address 40.0.0.5 255.255.255.0
no sh
exit
int fa 2/0
ip address 192.168.50.1 255.255.255.0
no sh
exit
int fa 2/1
ip address 192.168.60.1 255.255.255.0
no sh
exit

router ospf 5
network 40.0.0.5 0.0.0.255 area 10
network 192.168.50.1 0.0.0.255 area 10
network 192.168.60.1 0.0.0.255 area 10
exit


ip access-list standard SPEC_IP_LIST
permit 30.0.0.0 0.0.0.255
exit
int fa 2/0
ip access-group SPEC_IP_LIST out
exit

! ACL 1/0 arayüzüne uygulandığında ospf paketleri de engelleniyor






VPC1 : ip 192.168.10.5 255.255.255.0 192.168.10.1
VPC2 : ip 192.168.20.5 255.255.255.0 192.168.20.1
VPC3 : ip 192.168.30.5 255.255.255.0 192.168.30.1
VPC4 : ip 192.168.40.5 255.255.255.0 192.168.40.1
VPC5 : ip 192.168.50.5 255.255.255.0 192.168.50.1
VPC5 : ip 192.168.60.5 255.255.255.0 192.168.60.1


























