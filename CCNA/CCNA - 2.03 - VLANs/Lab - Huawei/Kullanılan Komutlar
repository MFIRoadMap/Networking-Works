traffic classifier C1
	if-match vlan 10
	quit

traffic behaviour B1
	remark 8021p 3
	quit

traffic policy P1
	classifier C1 behaviour B1
	quit

int ge 1/0/1
	traffic-policy p1 inbound
	quit












